-- HWID Whitelist

local HWID = game:GetService("RbxAnalyticsService"):GetClientId();
local WhitelistedHWIDs = {"b412583b-2c3f-4336-a259-1f4d02d4b371","a2b5807d-ba71-4cd2-9e04-3beacba7b223","425F660D-1DDD-4676-920A-E4558699B77A", ""}
local qNVAKkuwxNpqruLjSRHg = false

function CheckHWID(hwidval)
for _,whitelisted in pairs(WhitelistedHWIDs) do
 if hwidval == whitelisted then
     return true
 elseif hwidval ~= whitelisted then
     return false
       end
    end
end

qNVAKkuwxNpqruLjSRHg = CheckHWID(HWID)

if qNVAKkuwxNpqruLjSRHg == true then
 loadstring(game:HttpGet("https://raw.githubusercontent.com/Ngigerngieofoepdo/UpdatedPaidFarm/refs/heads/main/SlapFarmElgato"))()
end
